{
  "name": "janus-ai",
  "version": "1.0.0",
  "description": "An AI service rate-limit enforcer backed by Redis",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbeoris/janus-ai.git"
  },
  "keywords": [
    "rate-limit",
    "ai"
  ],
  "author": "jbeoris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbeoris/janus-ai/issues"
  },
  "homepage": "https://github.com/jbeoris/janus-ai#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "gpt-tokenizer": "^2.1.1",
    "lodash": "^4.17.21",
    "openai-chat-tokens": "^0.2.4",
    "redis": "^4.6.7"
  }
}
