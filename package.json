  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbeoris/janus-ai/issues"
  },
  "homepage": "https://github.com/jbeoris/janus-ai#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
